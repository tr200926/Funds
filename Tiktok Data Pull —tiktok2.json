        "jsonBody": "={ \"advertiser_id\": \"{{ $json.Advertiser_id }}\", \"report_type\": \"BASIC\", \"data_level\": \"AUCTION_CAMPAIGN\", \"dimensions\": [\"stat_time_day\",\"campaign_id\"], \"metrics\": [\"stat_cost\"], \"start_date\": \"2025-08-01\", \"end_date\":\"2025-08-26\", \"page_size\": 1000 }\n",
